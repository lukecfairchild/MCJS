{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
    	"include" : [
			"./MCJS/lib/globals/mc/events",
			"./MCJS/lib/globals/mc",
			"./MCJS/lib/globals",
			"./MCJS/lib/Class",
			"./MCJS/lib"
    	],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName"      : "MCJS",
        "footer"          : "",
        "copyright"       : "",
        "navType"         : "vertical",
        "theme"           : "spacelab", // spacelab yeti sandstone
        "linenums"        : true,
        "collapseSymbols" : false,
        "inverseNav"      : true
    }
}
